:plain
  if ("#{ @modal_id }".length > 0) {
    $("##{ @modal_id }").modal('hide');
    $('.modal-backdrop').remove();
    $('body').removeClass('modal-open');
  }

  $("#charterers").html(
    '#{escape_javascript(
       render partial: "/submissions/extended/forms/charterers/base")}'
  );

  $("#correspondent").html(
    '#{escape_javascript(
       render partial: "/submissions/extended/forms/correspondent/base")}'
  );
